<%- include('header') %>
  <div class="head">
    <h2>Open roles:</h2>
    <a href="/admin">Admin:</a>
  </div>
  <% categorias.forEach(categoria => { %> 
    <div>
      <h3><%- categoria.name %></h3>
      <ul>
        <% categoria.vagas.forEach( vaga => { %> 
          <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
        <% }) %>
      </ul>
    </div>
  <% }) %>

<%- include('footer') %>